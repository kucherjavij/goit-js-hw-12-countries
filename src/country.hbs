{{#each this}}
<div>
    <h2>
        {{name}}
    </h2>
    <div>
        <p>Capital:{{capital}}</p>
        <p>Population:{{population}}</p>
        <p>Languages:</p>
        <ul>
            
            {{#each languages}}
            <li>
{{name}}
        </li>
        {{/each}}
        </ul>
        <div>
            <img src="{{flag}}" alt="flag {{name}}">
        </div>
    </div>
</div>
{{/each}}